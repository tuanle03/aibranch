{
  "name": "@tuanle03/aibranch",
  "version": "1.1.0",
  "description": "AI-powered git branch name generator with smart detection",
  "type": "module",
  "bin": {
    "aibranch": "dist/cli.mjs",
    "aib": "dist/cli.mjs"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pkgroll --minify",
    "dev": "tsx src/cli.ts",
    "prepublishOnly": "npm run build",
    "version:patch": "npm version patch -m 'chore: bump version to %s'",
    "version:minor": "npm version minor -m 'chore: bump version to %s'",
    "version:major": "npm version major -m 'chore: bump version to %s'"
  },
  "keywords": [
    "ai",
    "git",
    "branch",
    "cli",
    "automation",
    "developer-tools",
    "conventional-commits"
  ],
  "author": "tuanle03",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tuanle03/aibranch.git"
  },
  "bugs": {
    "url": "https://github.com/tuanle03/aibranch/issues"
  },
  "homepage": "https://github.com/tuanle03/aibranch#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "cleye": "^2.0.0",
    "execa": "^7.0.0",
    "ini": "^3.0.1",
    "kolorist": "^1.8.0",
    "ai": "^6.0.13",
    "@ai-sdk/openai": "^3.0.5",
    "@ai-sdk/openai-compatible": "^2.0.4",
    "update-notifier": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/ini": "^1.3.31",
    "pkgroll": "^2.20.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
