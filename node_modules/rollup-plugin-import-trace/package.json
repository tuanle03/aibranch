    "vite",
    "vite-plugin",
    "import",
    "trace",
    "debug",
    "error"
  ],
  "license": "MIT",
  "repository": "privatenumber/rollup-plugin-import-trace",
  "funding": "https://github.com/privatenumber/rollup-plugin-import-trace?sponsor=1",
  "author": {
    "name": "Hiroki Osame",
    "email": "hiroki.osame@gmail.com"
  },
  "files": [
